{
  "name": "changelog",
  "displayName": "Changelog",
  "description": "This extension provides additional package.json changelog hover information.",
  "publisher": "jacobcofman",
  "version": "0.0.4",
  "author": {
    "name": "jacobcofman"
  },
  "homepage": "https://github.com/JCofman/Changelog",
  "bugs": {
    "url": "https://github.com/JCofman/Changelog/issues",
    "email": "cofman.jacob@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JCofman/Changelog.git"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#fff",
    "theme": "light"
  },
  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test-compile": "tsc -p ./",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.12",
    "@types/semver": "^6.0.1",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2",
    "vscode": "^1.1.35",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "semver": "^6.2.0"
  }
}
